int main()
{
    int b[50];
    while(scanf("%d",&n)!=EOF)
    {
        for (int i = 1; i <= n;i++)
        {
            scanf("%d", &b[i]);
        }
        int flag = 0;
        for (int i = 0; i <= 4799-n;i++)
        {
            flag = 0;
            for (int j = 1; j <= n;j++)
            {
                if(b[j]!=a[i+j-1])
                {
                    flag = 1;
                    break;
                }
            }
            if(flag==0)
            {
                printf("Yes\n");
                flag = 2;
                break;
            }
        }
        if(flag!=2)
            printf("No\n");
    }
}